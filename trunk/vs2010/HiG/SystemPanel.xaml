<UserControl x:Class="BioNex.Hig.SystemPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dig="clr-namespace:Asaasoft.DigitalMeter;assembly=Asaasoft.DigitalMeter"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <dig:DigitalMeter Precision="4" ScalingFactor="0" MeasurementUnit="G" Background="DarkOrange" Value="{Binding CurrentGs}" Margin="5" />
    </Grid>
</UserControl>
