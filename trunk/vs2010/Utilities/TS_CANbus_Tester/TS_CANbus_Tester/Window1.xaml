<Window x:Class="TS_CANbus_Tester.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Techosoft CANbus Tester" Height="355" Width="300">
    <StackPanel>
    <Grid>
        <Label Height="28" Margin="14.611,26.915,0,0" Name="SystecDeviceLabel" VerticalAlignment="Top" HorizontalContentAlignment="Right" HorizontalAlignment="Left" Width="98">Systec Device#</Label>
        <TextBox Height="37" Margin="127.654,18.456,63.058,0" Name="SystecDeviceTextBox" VerticalAlignment="Top" />
        <Label Height="28" Margin="14.611,73.824,0,0" Name="TSAxisLabel" VerticalAlignment="Top" HorizontalContentAlignment="Right" HorizontalAlignment="Left" Width="98">TS Axis#</Label>
        <TextBox Height="37" Margin="127.654,65.365,63.058,0" Name="TSAxisTextBox" VerticalAlignment="Top" />
        <Label HorizontalContentAlignment="Right" Margin="14.611,121.502,0,113.043" Name="NumMsgsLabel" HorizontalAlignment="Left" Width="98">Num Msgs:</Label>
        <TextBox Margin="127.654,112.274,63.058,113.043" Name="NumMessagesTextBox" />
        <Button Height="23" Margin="127.654,0,0,76.131" Name="RunTestButton" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="87.29" Click="RunTestButton_Click">Run Test</Button>
        <Label Height="28" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Margin="7.69,0,0,32.298" Name="ElapsedTimeLabel" VerticalAlignment="Bottom" Width="109.198">Elapsed Time (ms)</Label>
        <TextBox Height="37" HorizontalAlignment="Left" Margin="19.225,0,0,0.769" Name="ElapsedTimeMsTextBox" VerticalAlignment="Bottom" Width="88" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
        <Label Height="28" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Margin="0,0,10.766,31.529" Name="MsgPerSecLabel" VerticalAlignment="Bottom" Width="109.198">Messages/sec</Label>
        <TextBox Height="37" HorizontalAlignment="Right" Margin="0,0,20.763,0" Name="MsgPerSecTextBox" VerticalAlignment="Bottom" Width="88" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
    </Grid>
        <Separator></Separator>
        <Grid>
            <Label Height="28" HorizontalAlignment="Left" HorizontalContentAlignment="Center">Maximum OS Timer Resolution:</Label>
            <Label Height="28" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Name="MaxResLabel">x.xxx ms</Label>
        </Grid>
        <Grid>
            <Label Height="28" HorizontalAlignment="Left" HorizontalContentAlignment="Center">Current OS Timer Resolution: </Label>
            <Label Height="28" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Name="CurResLabel">x.xxx ms</Label>
        </Grid>
    </StackPanel>
</Window>
