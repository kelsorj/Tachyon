<Window x:Class="PlateStorageStackerTestApplication.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="PlateStorageStacker Test Application" Width="283" Height="181"
    Closing="Window_Closing" >
    <StackPanel>
        <Button Margin="3" Click="Connect_Click">Connect</Button>
        <Button Margin="3" Click="Upstack_Click">Upstack</Button>
        <Button Margin="3" Click="Downstack_Click">Downstack</Button>
        <Button Margin="3" Click="IllegalCommand_Click">Illegal command</Button>
        <Button Margin="3" Click="Disconnect_Click">Disconnect</Button>
    </StackPanel>
</Window>
