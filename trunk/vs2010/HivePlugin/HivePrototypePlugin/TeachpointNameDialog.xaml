<Window x:Class="BioNex.HivePrototypePlugin.TeachpointNameDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Save New Teachpoint" Height="98" Width="222" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <TextBox Name="text_teachpoint" Grid.Row="0" VerticalContentAlignment="Center" Text="{Binding TeachpointName, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged }" ></TextBox>
        <Button Grid.Row="1" Margin="3" Click="Save_Click" IsDefault="True">Save</Button>
    </Grid>
</Window>
