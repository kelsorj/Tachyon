<Window x:Class="HivePluginTestApplication.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Hive Plugin Test Application" Height="300" Width="600">
    <DockPanel>
        <Button DockPanel.Dock="Top" Click="Button_Click" Margin="3">Show Diagnostics</Button>
        <GroupBox DockPanel.Dock="Bottom" Header="Errors">
            <ListBox ItemsSource="{Binding Errors}" />
        </GroupBox>
    </DockPanel>
</Window>
