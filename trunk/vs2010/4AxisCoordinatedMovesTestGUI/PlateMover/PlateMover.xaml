<UserControl x:Class="PlateMover.UserControl1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="81" Width="127">
    <Canvas Name="plate">
        <Canvas.RenderTransform>
            <TransformGroup>
                <RotateTransform Angle="0" CenterX="64" CenterY="41"></RotateTransform>
                <TranslateTransform Y="0"></TranslateTransform>
            </TransformGroup>
        </Canvas.RenderTransform>
        <Rectangle Canvas.Left="0" Canvas.Top="0" Stroke="Black" Height="80" Width="127" Fill="WhiteSmoke"></Rectangle>
        <UniformGrid Name="WellGrid" Rows="8" Columns="12">
        </UniformGrid>
    </Canvas>
</UserControl>
