			<TextBlock Grid.Row="1" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Top" Text="Manufacturer" d:LayoutOverrides="Width" Margin="0,-0.029,-69.646,0"/>
			<TextBlock Grid.Row="2" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Top" Text="Number of wells" d:LayoutOverrides="Width" Margin="0,-0.029,-69.646,0"/>
