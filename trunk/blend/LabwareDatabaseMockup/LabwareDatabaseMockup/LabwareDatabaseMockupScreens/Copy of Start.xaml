			<TabItem Style="{DynamicResource TabItem-Sketch}" Header="Rainin">
				<ListBox Style="{DynamicResource ListBox-Sketch}">
					<ListBoxItem Style="{DynamicResource ListBoxItem-Sketch}" Content="Rainin 96 300uL tip box"/>
				</ListBox>
			</TabItem>
		</TabControl>
		<TextBlock HorizontalAlignment="Left" Margin="8,0,0,171.243" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Bottom" Text="Properties"/>
