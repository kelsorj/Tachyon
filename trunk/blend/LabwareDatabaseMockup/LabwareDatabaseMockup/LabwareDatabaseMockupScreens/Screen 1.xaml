<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:ps="clr-namespace:Microsoft.Expression.Prototyping.SketchControls;assembly=Microsoft.Expression.Prototyping.SketchControls" x:Name="Screen_1_Name"
	x:Class="LabwareDatabaseMockupScreens.Screen_1"
	Width="640" Height="480">
	<UserControl.Resources>
		<Storyboard x:Key="OnSelectionChanged1"/>
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" Background="White">
		<TextBlock HorizontalAlignment="Left" Margin="8,8,0,0" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Top" Width="106" Height="18" Text="Labware Editor"/>
		<TextBlock HorizontalAlignment="Left" Margin="305,58,0,0" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Top" Width="5" Height="0" Text="v"/>
		<TextBlock HorizontalAlignment="Left" Margin="63,127,0,0" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Top" Width="3" Height="0" Text="v"/>
		<TextBlock HorizontalAlignment="Left" Margin="8,38.243,0,0" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Top" Text="Available Labware"/>
		<TextBlock HorizontalAlignment="Left" Margin="163,38.243,0,0" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Top" RenderTransformOrigin="0.97,0.354" Text="Sort by:"/>
		<ComboBox x:Name="comboBox" Margin="215.443,29.243,266.557,0" Style="{DynamicResource ComboBox-Sketch}" VerticalAlignment="Top" Height="28.756" SelectedIndex="0">
			<ComboBoxItem Content="Number of wells"/>
			<ComboBoxItem Content="Manufacturer"/>
		</ComboBox>
		<TabControl Margin="8,62,266.557,195" Style="{DynamicResource TabControl-Sketch}">
			<TabItem Style="{DynamicResource TabItem-Sketch}" Header="96">
				<ListBox Style="{DynamicResource ListBox-Sketch}">
					<ListBoxItem Style="{DynamicResource ListBoxItem-Sketch}" Content="Greiner 96 round well flat bottom clear"/>
					<ListBoxItem Style="{DynamicResource ListBoxItem-Sketch}" Content="NUNC 96 deepwell"/>
					<ListBoxItem Style="{DynamicResource ListBoxItem-Sketch}" Content="Rainin 96 300uL tip box"/>
				</ListBox>
			</TabItem>
			<TabItem Style="{DynamicResource TabItem-Sketch}" Header="384"/>
		</TabControl>
		<TextBlock HorizontalAlignment="Left" Margin="8,0,0,171.243" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Bottom" Text="Properties"/>
		<ps:SketchRectangleUC Margin="10,0,267,11" Style="{DynamicResource Rectangle-Sketch}" VerticalAlignment="Bottom" Height="153"/>
		<Button HorizontalAlignment="Right" Margin="0,0,95,140.536" Style="{DynamicResource Button-Sketch}" VerticalAlignment="Bottom" Width="158" Content="Create New"/>
		<Button HorizontalAlignment="Right" Margin="0,0,95,85.608" Style="{DynamicResource Button-Sketch}" VerticalAlignment="Bottom" Width="158" Content="Delete"/>
		<Button HorizontalAlignment="Right" Margin="0,0,95,113.072" Style="{DynamicResource Button-Sketch}" VerticalAlignment="Bottom" Width="158" Content="Rename"/>
		<Button HorizontalAlignment="Right" Margin="0,0,95,58.144" Style="{DynamicResource Button-Sketch}" VerticalAlignment="Bottom" Width="158" Content="Copy"/>
		<Button HorizontalAlignment="Right" Margin="0,0,95,30.68" Style="{DynamicResource Button-Sketch}" VerticalAlignment="Bottom" Width="158" Content="Apply Changes"/>
	</Grid>
</UserControl>